{
    "name": "n4m-ward/onboarding_project",
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/phpinsights": "^2.3"
    },
    "config": {
        "platform": {
            "php": "8.1.1"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Onboarding\\": "app/",
            "Onboarding\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "require": {
        "tightenco/collect": "^9.12",
        "fakerphp/faker": "^1.19"
    }
}
